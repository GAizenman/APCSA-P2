import static java.lang.System.*;

public class Name
{
	private String name;
	int spa = 0;

	public Name()
	{


	}

	public Name(String s)
	{
		
		

	}

   public void setName(String s)
   {
	   
	   name = s;
	   
   }

	public String getFirst()
	{

		while ( !name.substring(spa, spa).equals(" ")){
			spa += 1;
		}
		
		String firstName = name.substring(0, spa);
		
		return firstName;
	}

	public String getLast()
	{
		String lastName = name.substring(spa, name.length());
		return lastName;
	}

 	/*public String toString()
 	{
 		return "";
	}*/
}