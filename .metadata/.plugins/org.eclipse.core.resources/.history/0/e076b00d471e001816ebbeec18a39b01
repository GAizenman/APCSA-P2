	//first method
	public static boolean isGoofy (int num){
		int num1 = (num / 10);
		int num2 = num - 10;
		int numTot = 0;
		for (int i = 0; i <= 2; i++){
			if (num%num1 == 0){
				numTot += num1;
			}
			if (num%num2 == 0){
				numTot += num2;
			}
		}
		
		if (numTot%2 == 0){
			return false;
		}
		
		return true;
	}
	
